   ],
   "source": [
    "def f(x):\n",
    "    return x**2\n",
    "\n",
    "epsilon = 1e-4\n",
    "x = 1.5\n",
    "\n",
    "numericGrad = (f(x+epsilon) - f(x-epsilon)) / (2*epsilon)\n",
    "\n",
    "numericGrad, 2*x"
   ]
